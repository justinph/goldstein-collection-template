<div class="row home-objects">
    <section class="col-md-5 col-md-offset-1 object-of-week">
    {{> object-week}}
    </section>

    <section class="col-md-5 featured-objects">
        {{#if home.featured-objects}}
        <h2>Featured Objects</h2>

            {{#if home.featured-objects.items}}
                <ul class="featured-objects-list">
                {{#each home.featured-objects.items}}
                    {{#if thumb-url}}
                    <li><a href="{{url}}" style="background-image: url({{thumb-url}})">
                    {{!-- <img src="{{thumb-url}}" alt="" /> --}}
                    </a></li>
                    {{/if}}
                {{/each}}
                </ul>
            {{/if}}

            {{#if home.featured-objects.archive-url}}
            <a href="{{home.featured-objects.archive-url}}" class="btn btn-primary">Featured object archive <span class="glyphicon glyphicon-triangle-right" aria-hidden="true"></span></a>
            {{/if}}

        {{/if}}

    </section>
</div>
