
<div class='object-detail'>

    <div class="row">
        <div class="col-md-12">
            <h1>{{object-detail.title}}</h1>
        </div>
    </div>


    <div class="row">

     <div class="col-md-6 col-md-push-6">


        <h2 class="sr-only">Object Images</h2>

            <a href="{{object-detail.images.0.full-url}}"><img src="{{object-detail.images.0.med-url}}" alt="" class="img-responsive"/></a>


            <ul class="object-images-list">
            {{#each object-detail.images}}
                <li><a href="{{full-url}}" data-med-url="{{med-url}}"><img src="{{thumb-url}}" alt="{{title}}" /></a></li>
            {{/each}}
            </ul>
        </div>

        <div class="col-md-6 col-md-pull-6">
            <h2 class="sr-only">Object Information</h2>
            <dl>
            {{#each object-detail.information}}
              <dt>{{@key}}</dt>
              <dd>{{this}}</dd>
            {{/each}}
            </dl>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6 related-topics">
            <h2>Explore related topics</h2>
            <ul class="related-topics-list">
            {{#each object-detail.related-topics}}
                <li><a href="{{url}}"><span class="label label-default">{{title}}</span></a></li>
            {{/each}}
            </ul>
        </div>

        <div class="col-md-6 related-objects">
            <h2>Related objects</h2>
            <ul class="related-objects-list">
            {{#each object-detail.related-objects}}
                <li><a href="{{url}}">

                <h3>{{title}}</h3>
                    {{designer}}
                    {{[date made]}}
                </a></li>
            {{/each}}
            </ul>
       </div>
    </div>

</div> <!-- end object-detail -->
